<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.dao.ShopDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.entity.ShopEntity" id="shopMap">
        <result property="id" column="id"/>
        <result property="shopName" column="shop_name"/>
        <result property="shopAddr" column="shop_addr"/>
        <result property="shopLongitude" column="shop_longitude"/>
        <result property="shopLatitude" column="shop_latitude"/>
        <result property="meituanShopId" column="meituan_shop_id"/>
        <result property="dianpingShopId" column="dianping_shop_id"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateBy" column="update_by"/>
    </resultMap>

    <select id="findAll" resultType="io.renren.dto.ShopDto">
        select id,shop_name,shop_latitude,shop_longitude from tb_shop
    </select>


</mapper>